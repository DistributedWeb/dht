{
  "name": "@dwebswarm/dht",
  "version": "3.0.0",
  "description": "The DHT backing the DWebSwarm stack",
  "main": "index.js",
  "dependencies": {
    "@dwebswarm/dwebsign": "git://github.com/DistributedWeb/dwebsign.git#8add9a90aa4999b8c61ad02f299b9d20022b0750",
    "dht-rpc": "^4.1.6",
    "end-of-stream": "^1.4.1",
    "hashlru": "^2.3.0",
    "ipv4-peers": "^2.0.0",
    "protocol-buffers-encodings": "^1.1.0",
    "record-cache": "^1.1.0",
    "sodium-universal": "^2.0.0"
  },
  "devDependencies": {
    "get-port": "^5.0.0",
    "nonsynchronous": "^1.0.1",
    "protocol-buffers": "^4.1.0",
    "standard": "^13.1.0",
    "tap": "^14.5.0"
  },
  "bin": {
    "dwebswarm-dht": "./bin.js"
  },
  "scripts": {
    "test": "tap  -R classic test/*.test.js  && standard --fix",
    "cov": "tap -R classic --100 --coverage-report=html test/*.test.js",
